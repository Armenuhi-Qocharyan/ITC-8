{
    "name": "intrusion-detector",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "npm run _server:run \"ng serve\"",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "_server:build": "tsc -p ./server",
        "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",


        "start-dev": "NODE_ENV=development concurrently \"npm run _dev:start-server\" \"npm run _dev:start-web\"",
        "_dev:start-web": "ng serve --dev",
        "_dev:start-server": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"npm run _dev:server-run\"",
        "_dev:server-run": "nodemon dist/server/bin/www.js",

        "start-prod": "NODE_ENV=production concurrently \"npm run _prod:start-server\" \"npm run _prod:start-web\"","install": "",
        "_prod:start-web": "ng serve --prod",
        "_prod:start-server": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"npm run _prod:server-run\"",
        "_prod:server-run": "nodemon dist/server/bin/www.js"
    },

